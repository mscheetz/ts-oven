{
  "name": "ts-oven",
  "version": "0.0.0",
  "scripts": {
    "gogo": "concurrently \"npm run start\" \"npm run server:start\"",
    "build": "npm run config -- --environment=prod && ng build --aot --prod --output-hashing=all --build-optimizer=false",
    "devbuild": "npm run config -- --environment=dev && ng build --output-hashing=all",
    "server:start": "cd ./server && ts-node index.ts",
    "config": "ts-node ./scripts/config.ts",
    "ng": "ng",
    "start": "npm run devbuild && ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "@types/dotenv": "^8.2.0",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^15.0.7",
    "angular2-qrcode": "^2.0.3",
    "axios": "^0.21.0",
    "btc-xpub-address": "^0.1.5",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "ngx-cookie-service": "^10.1.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "winston": "^3.3.3",
    "yargs": "^16.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.64",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
